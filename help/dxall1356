,  #!Ldxscv,dxall1372 h DXSetComponentValue  #!EL  ,  #!Ldxswp,dxall1397 h DXSwap  #!EL  #!EF #!N #!N  #!Lctmn,dxall1089 h Component Manipulation  #!EL  . #!N #!N 
